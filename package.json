{
  "name": "zel",
  "version": "0.1.0",
  "description": "zel is small, and simple command-line tool that helps kickstart new projects.",
  "main": "lib/index.js",
  "bin": {
    "zel": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 7.4"
  },
  "scripts": {
    "build": "flow-remove-types --pretty src/ --out-dir lib/",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint": "prettier --parser=flow --single-quote --trailing-comma=es5 --print-width=90 --tab-width=4 --write 'src/*.js' 'index.js'",
    "postinstall": "npm run flow-typed",
    "precommit": "npm run lint",
    "prepublish": "npm run build",
    "jest": "jest --coverage",
    "test": "npm run lint && npm run jest"
  },
  "keywords": [],
  "author": "Vu Tran <vu@vu-tran.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "cache-conf": "^0.5.0",
    "caporal": "^0.5.0",
    "chalk": "^1.1.3",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "coveralls": "^2.13.0",
    "flow-bin": "^0.43.1",
    "flow-remove-types": "^1.2.0",
    "flow-typed": "^2.0.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "prettier": "^0.22.0"
  }
}
